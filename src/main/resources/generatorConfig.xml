<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD 

 MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd 

" >
<generatorConfiguration>
	<properties resource="application.properties" />
	<context id="mysql">
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
			<property name="mappers" value="tk.mybatis.mapper.common.Mapper" />
			<!-- caseSensitive默认false，当数据库表名区分大小写时，可以将该属性设置为true -->
			<property name="caseSensitive" value="true" />
		</plugin>

		<jdbcConnection driverClass="${jdbc.driver}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}" />

		<javaTypeResolver>
			<!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="okkpp.model.economics"
			targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="okkpp.dao.economics"
			targetProject="src/main/java" />
		<javaClientGenerator targetPackage="okkpp.dao.economics"
			targetProject="src/main/java" type="XMLMAPPER" />
		<table tableName="t_economics_final_consumption"
			domainObjectName="FinalConsumption" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_gdp"
			domainObjectName="Gdp" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_gdppc"
			domainObjectName="Gdppc" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_gnipc"
			domainObjectName="Gnipc" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_growth_rate_gdp"
			domainObjectName="GrowthRateGdp" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_growth_rate_gdppc"
			domainObjectName="GrowthRateGdppc" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_indicators_of_na"
			domainObjectName="IndicatorsOfNa" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_percentage_of_agriculture"
			domainObjectName="PercentageOfAgriculture" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_percentage_of_indices"
			domainObjectName="PercentageOfIndices" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_percentage_of_service"
			domainObjectName="PercentageOfService" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_rate_of_consumption"
			domainObjectName="RateOfConsumption" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_rate_of_formation"
			domainObjectName="RateOfFormation" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />
		<table tableName="t_economics_share_of_formation"
			domainObjectName="ShareOfFormation" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" schema="${jdbc.username}" />

	</context>
</generatorConfiguration>
