<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<properties resource="application.properties" />
	<context id="mysql">
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
			<property name="mappers" value="tk.mybatis.mapper.common.Mapper" />
			<!-- caseSensitive默认false，当数据库表名区分大小写时，可以将该属性设置为true -->
			<property name="caseSensitive" value="true" />
		</plugin>

		<jdbcConnection driverClass="${jdbc.driver}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}" />

		<javaTypeResolver>
			<!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="okkpp.model.culture"
			targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="okkpp.dao.culture"
			targetProject="src/main/java" />
		<javaClientGenerator targetPackage="okkpp.dao.culture"
			targetProject="src/main/java" type="XMLMAPPER" />
		<table tableName="t_culture_adult_literacy_rate" domainObjectName="AdultLiteracyRate"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_expenditure_student_gdp_rate" domainObjectName="ExpenditureStudentGDPRate"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_health_total_rate" domainObjectName="HealthTotalRate"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_hightechnology_rate" domainObjectName="HightechnologyRate"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_hospital" domainObjectName="Hospital"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_patent_applications" domainObjectName="PatentApplications"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_researchers_and_technicians" domainObjectName="ResearchersAndTechnicians"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_reserch_spending_rate" domainObjectName="ReserchSpendingRate"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_school_enrollment_ratio" domainObjectName="SchoolEnrollmentRatio"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
		<table tableName="t_culture_water_people_rate" domainObjectName="WaterPeopleRate"
			enableCountByExample="false" enableDeleteByExample="false" 
			enableSelectByExample="false" enableUpdateByExample="false" 
			schema="${jdbc.username}"/>
	</context>
</generatorConfiguration>